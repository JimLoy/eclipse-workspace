#include <iostream>
using namespace std;

int main() {
	int side1;
	int side2;
	int side3;
	int totalE = 0;
	int totalI = 0;
	int totalS = 0;

	cin >> side1;
	cin >> side2;
	cin >> side3;

	while(side1 != 0 && side2 != 0 && side3 != 0 ){
		if(side1 == side2){
			if(side1 == side3){
				totalE++;
			} else {
				totalI++
			}
		}



		cin >> side1;
		cin >> side2;
		cin >> side3;
	}
	return 0;
}
