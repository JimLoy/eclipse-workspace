#include <iostream>
using namespace std;

int main() {
	int N;
	int M;
	int totalM;
	int multiple;
	int result;

	cin >> N;
	cin >> M;

	totalM = N / M;



	cout << "Total of multiples: " << totalM << endl;

	while(totalM > 0){
		result =  multiple * M;
		cout << "Multiples: " << result << endl;
		totalM--;
	}

	return 0;
}

